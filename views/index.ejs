  <script src="/scripts/bootstrap.min.js"></script>
  <script src="/scripts/theater.min.js"></script>
  <script>
    $(function() {
      var theater = theaterJS()

      theater
        .addActor('js-title')      
        .addScene('js-title:你在或不在，未来已来！', 500)
    })
  </script>
