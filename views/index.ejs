  <link rel="stylesheet" href="/css/page/comment.css">
  <link rel="stylesheet" href="/css/page/index.css">
</head>
<body>
  <%- include header.ejs %>

  <div class="container">
    <h1 class="search-title" id="js-title"></h1>
    <form action="/search" method="get">
      <div class="input-group search-box">
        <input type="text" class="form-control" name="q" placeholder="大家都在搜人工智能">
        <span class="input-group-btn">
          <button class="btn btn-primary btn-lg" type="submit">搜索</button>
        </span>
      </div>
    </form>
    <div class="hot-box">
      热门搜索：
      <a href="/search?q=人工智能" class="label label-danger">人工智能</a>
      <a href="/search?q=共享单车" class="label label-danger">共享单车</a>
